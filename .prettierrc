{
  "semi": false,
  "trailingComma": "es5",
  "singleQuote": false,
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": ["^@/app/(.*)$", "^@/pages/(.*)$", "^@/shared/components/(.*)$", "^@/shared/utils/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
